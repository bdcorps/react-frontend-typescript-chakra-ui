{
  "name": "vite-project",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "setupFiles": [
      "raf/polyfill"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^1.7.3",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "enzyme": "^3.11.0",
    "framer-motion": "^4.1.17",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.10",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.0.7",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "enzyme-to-json": "^3.6.2",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "raf": "^3.4.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.4.4",
    "vite": "^2.7.2"
  }
}
